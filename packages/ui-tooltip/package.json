{
  "name": "@devlaunch/ui-tooltip",
  "version": "1.0.0",
  "description": "The bootstrap tooltip component created with styled-components",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "private": false,
  "scripts": {
    "start": "react-scripts start",
    "build-examples": "react-scripts build",
    "test": "react-scripts test",
    "build": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "prepublish": "yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "author": "Devlaunch <devlaunchorg@gmail.com>",
  "license": "MIT",
  "keywords": [
    "components",
    "react",
    "react-component",
    "ui",
    "styled-components",
    "tooltip",
    "bootstrap",
    "presentational",
    "stateless"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlaunch/dev-starter.git"
  },
  "bugs": {
    "url": "https://github.com/devlaunch/dev-starter/issues"
  },
  "homepage": "https://github.com/devlaunch/dev-starter",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "jest-styled-components": "^6.2.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "react-test-renderer": "^16.5.2",
    "styled-components": "^3.4.10",
    "styled-system": "^3.1.6"
  },
  "peerDependencies": {
    "react": "^16.5.2",
    "styled-components": "^3.4.10",
    "styled-system": "^2.1.0"
  }
}
