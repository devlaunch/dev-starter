{
  "name": "dev-starter",
  "version": "0.0.1",
  "description": "Javascript application development starter",
  "main": "index.js",
  "repository": "https://github.com/phaneendra/dev-starter.git",
  "author": "phaneendra <phaneendra.krishnareddy@gmail.com>",
  "license": "MIT",
  "private": true,
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "scripts": {
    "setup": "lerna clean --yes && yarn install",
    "lerna": "lerna",
    "lernawiz": "lerna-wizard",
    "benchmark": "babel-node benchmark",
    "benchmark:watch": "nodemon src/index.js --exec babel-node benchmark",
    "changelog": "standard-changelog --preset angular-emoji --outfile changelog.md",
    "lint": "eslint packages",
    "format": "prettier-eslint --write \"packages/**/*.js\"",
    "lint:travis": "travis-lint .travis.yml",
    "lint:markdown": "markdownlint readme.md",
    "lint:markdown-links": "markdown-link-check readme.md",
    "precommit": "yarn run format",
    "commit": "git add --all && git status && git-cz",
    "commitmsg": "commitlint -e",
    "prerelease":
      "yarn run setenv:prod && yarn run clean && yarn run build && yarn run update-dependencies && yarn run lint && yarn run test && yarn run coverage",
    "release": "standard-version --no-verify",
    "postrelease": "greenkeeper-postpublish && yarn run setenv:dev",
    "security:test": "snyk test",
    "security:wizard": "snyk wizard",
    "security:protect": "snyk protect",
    "security:monitor": "snyk monitor",
    "update-dependencies": "yarn upgrade",
    "setenv:dev": "yarn config set production false",
    "setenv:prod": "yarn config set production true",
    "tasks": "ntl",
    "online": "git push --follow-tags origin master; yarn publish"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "@commitlint/config-lerna-scopes": "^6.0.2",
    "babel-cli": "^7.0.0-beta.3",
    "babel-eslint": "^8.2.1",
    "babel-minify": "^0.2.0",
    "babel-preset-env": "2.0.0-beta.2",
    "commitizen": "^2.9.6",
    "cz-customizable": "^5.2.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lerna": "^2.8.0",
    "lerna-wizard": "^1.0.9",
    "nodemon": "^1.14.11",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.0"
  },
  "workspaces": ["packages/*"]
}
